<runtime ram="32M" caps="1000" binary="init">

	<requires> <timer/> </requires>

	<fail after_seconds="30"/>
	<succeed>child "test-utf8" exited with exit value 0</succeed>
	<fail>Error: </fail>

	<content>
		<rom label="ld.lib.so"/>
		<rom label="test-utf8"/>
	</content>

	<config>
		<parent-provides>
			<service name="ROM"/>
			<service name="LOG"/>
			<service name="CPU"/>
			<service name="PD"/>
		</parent-provides>
		<default-route>
			<any-service> <any-child/> <parent/> </any-service>
		</default-route>
		<default caps="100"/>
		<start name="test-utf8">
			<resource name="RAM" quantum="1M"/>
		</start>
	</config>
</runtime>
